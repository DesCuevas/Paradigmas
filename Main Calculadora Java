public class Main {
    public static void main(String[] args) {

        Calculadora calcuA = new Calculadora();

        Calculadora calcuB = new Calculadora(22, 8.4f, 5.18);

        System.out.println("suma de numeros tipo enteros -> " + calcuA.suma(5, 3));

        System.out.println("resta de numeros tipo flotantes -> " + calcuB.resta(5.2f, 1.1f));

        System.out.println("multiplicación de numeros tipo double -> " + calcuA.multi(2.5, 3.5));

        System.out.println("división de numeros tipo enteros -> " + calcuB.division(15, 3));

        System.out.println("módulo de numeros tipo enteros -> " + calcuA.modulo(10, 3));
    }
}

